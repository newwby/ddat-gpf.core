[application]

config/name="#[ddat-gpf.core] Prototyping Framework Core"
config/description="workspace to focus on developing modular frameworks for godot projects"
run/main_scene="res://_dev/placeholder_main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="ddat_framework"
config/icon="res://_dev/ddat_icon.png"

[autoload]

GlobalLog="*res://src/ddat-gpf/core/autoload/global_log.gd"
GlobalData="*res://src/ddat-gpf/core/autoload/global_data.gd"
GlobalDebug="*res://src/ddat-gpf/core/autoload/global_debug.tscn"
GlobalFunc="*res://src/ddat-gpf/core/autoload/global_functions.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=1920
window/size/height=1080

[editor]

script_templates_search_path="res://src/ddat-gpf/core/templates"

[input]

