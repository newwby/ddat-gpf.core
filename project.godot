; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "ErrorCodes",
"language": "GDScript",
"path": "res://src/ddat-gpf/core/classes/class_error_codes.gd"
}, {
"base": "Resource",
"class": "GameDataContainer",
"language": "GDScript",
"path": "res://_testenv/test_classes/class_game_data_container.gd"
}, {
"base": "Node",
"class": "GameGlobal",
"language": "GDScript",
"path": "res://src/ddat-gpf/core/classes/class_global.gd"
}, {
"base": "Node",
"class": "UnitTest",
"language": "GDScript",
"path": "res://_testenv/test_classes/class_unit_test.gd"
}, {
"base": "UnitTest",
"class": "UnitTestGetDirPaths",
"language": "GDScript",
"path": "res://_testenv/core_global_data/test_get_dir_paths.gd"
} ]
_global_script_class_icons={
"ErrorCodes": "",
"GameDataContainer": "",
"GameGlobal": "",
"UnitTest": "",
"UnitTestGetDirPaths": ""
}

[application]

config/name="#[ddat-gpf.core] Prototyping Framework Core"
config/description="workspace to focus on developing modular frameworks for godot projects"
run/main_scene="res://_dev/placeholder_main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="ddat_framework"
config/icon="res://_dev/ddat_icon.png"

[autoload]

GlobalLog="*res://src/ddat-gpf/core/autoload/global_log.gd"
GlobalData="*res://src/ddat-gpf/core/autoload/global_data.gd"
GlobalDebug="*res://src/ddat-gpf/core/autoload/global_debug.tscn"
GlobalFunc="*res://src/ddat-gpf/core/autoload/global_functions.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=1920
window/size/height=1080

[editor]

script_templates_search_path="res://_dev/script_templates"

[logging]

file_logging/enable_file_logging=true

[network]

limits/debugger_stdout/max_chars_per_second=10000

[rendering]

environment/default_environment="res://_dev/default_env.tres"
